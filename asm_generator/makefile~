asmGenerate: comp
	./comp 1 > ass4_15CS10045_quads1.out
	./comp 2 > ass4_15CS10045_quads2.out
	./comp 3 > ass4_15CS10045_quads3.out
	./comp 4 > ass4_15CS10045_quads4.out
	./comp 5 > ass4_15CS10045_quads5.out

comp: lex.yy.o y.tab.o ass4_15CS10045_translator.o ass4_15CS10045_target_translator.o
	g++ lex.yy.o y.tab.o ass4_15CS10045_translator.o \
	ass4_15CS10045_target_translator.o -lfl -o comp

ass4_15CS10045_target_translator.o: ass4_15CS10045_target_translator.cxx
	g++ -c ass4_15CS10045_target_translator.cxx

ass4_15CS10045_translator.o: ass4_15CS10045_translator.cxx ass4_15CS10045_translator.h
	g++ -c ass4_15CS10045_translator.h
	g++ -c ass4_15CS10045_translator.cxx

lex.yy.o: lex.yy.c
	g++ -c lex.yy.c

y.tab.o: y.tab.c
	g++ -c y.tab.c

lex.yy.c: ass4_15CS10045.l y.tab.h ass4_15CS10045_translator.h
	flex ass4_15CS10045.l

y.tab.c: ass4_15CS10045.y
	yacc -dtv ass4_15CS10045.y -W

y.tab.h: ass4_15CS10045.y
	yacc -dtv ass4_15CS10045.y -W
	
clean:
	rm lex.yy.c y.tab.c y.tab.h lex.yy.o y.tab.o ass4_15CS10045_translator.o test1 test2 test3 test4 test5 y.output comp ass4_15CS10045_target_translator.o libass2_14CS10006.a ass4_15CS10045_1.o ass2_14CS10006.o ass4_15CS10045_2.o ass4_15CS10045_3.o ass4_15CS10045_4.o ass4_15CS10045_5.o

clean1:
	rm test1 ass4_15CS10045_1.o libass2_14CS10006.a ass2_14CS10006.o

#Test files
test1: ass4_15CS10045_1.o libass2_14CS10006.a
	gcc ass4_15CS10045_1.o -o test1 -L. -lass2_14CS10006
ass4_15CS10045_1.o: myl.h
	gcc -c ass4_15CS10045_1.s

test2: ass4_15CS10045_2.o libass2_14CS10006.a
	gcc ass4_15CS10045_2.o -o test2 -L. -lass2_14CS10006
ass4_15CS10045_2.o: myl.h
	gcc -Wall -c ass4_15CS10045_2.s

test3: ass4_15CS10045_3.o libass2_14CS10006.a
	gcc ass4_15CS10045_3.o -o test3 -L. -lass2_14CS10006
ass4_15CS10045_3.o: myl.h
	gcc -Wall -c ass4_15CS10045_3.s

test4: ass4_15CS10045_4.o libass2_14CS10006.a
	gcc ass4_15CS10045_4.o -o test4 -L. -lass2_14CS10006
ass4_15CS10045_4.o: myl.h
	gcc -Wall -c ass4_15CS10045_4.s

test5: ass4_15CS10045_5.o libass2_14CS10006.a
	gcc ass4_15CS10045_5.o -o test5 -L. -lass2_14CS10006
ass4_15CS10045_5.o: myl.h
	gcc -Wall -c ass4_15CS10045_5.s


libass2_14CS10006.a: ass2_14CS10006.o
	ar -rcs libass2_14CS10006.a ass2_14CS10006.o

ass2_14CS10006.o: ass2_14CS10006.c myl.h
	gcc -Wall -c ass2_14CS10006.c